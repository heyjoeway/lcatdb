$(() => {
    let queryObj = urlQueryObj(location.toString());
    $('#remove').click(function() {
        $('#step-1').hide();
        $('#loading').show();

        window.parent.postMessage('modal.lock', '*');

        $.post('./removeSensorDo',
            {
                "sid": queryObj.sid
            },
            (data, success) => {
                window.parent.postMessage('modal.done', '*');
            }
        );
    });
});