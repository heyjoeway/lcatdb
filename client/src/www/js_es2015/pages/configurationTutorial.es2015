$(function() {
    let hasTemperature = false;
    let hasDepth = false;

    $('.sensor').each(function() {
        let type = $(this).data('type');
        hasTemperature |= type == "temperature";
        hasDepth |= type == "depth";
    });

    if (hasTemperature && hasDepth) {
        $('#done').removeAttr('disabled').attr(
            'href',
            './reading?quick=true'
        );
    }
});