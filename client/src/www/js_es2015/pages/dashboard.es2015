LcatDB.pages.dashboard = {};

LcatDB.pages.dashboard.init = function() {
    let queryObj = LcatDB.urlQueryObj(window.location.href);

    ["verifySuccess", "verifyFailure"].forEach(function(key) {
        if (queryObj[key]) $('#' + key).show();
    });

    LcatDB.updateNavbar();
};

$('document').ready(function() { LcatDB.pages.dashboard.init() });